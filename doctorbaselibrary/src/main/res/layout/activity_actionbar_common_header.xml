<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:paddingLeft="6dp"
             android:paddingRight="6dp"
             android:background="@color/main_background_color">

<cn.demomaster.huan.quickdeveloplibrary.widget.ImageTextView
        android:id="@+id/it_actionbar_common_left"
        android:layout_width="@dimen/activity_actionbar_button_width"
        android:layout_height="40dp"
        android:padding="10dp"
        android:textColor="@color/main_color"
        android:textSize="@dimen/quickdev_title_text_size"
        android:gravity="center_vertical"
        android:src="@mipmap/ic_action_back" />

<cn.demomaster.huan.quickdeveloplibrary.widget.ImageTextView
        android:id="@+id/it_actionbar_common_title"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:gravity="center"
        android:layout_gravity="center"
        android:singleLine="true"
        android:text=""
        android:textColor="@color/main_color"
        android:textSize="@dimen/quickdev_title_text_size"
        android:textStyle="bold" />


    <cn.demomaster.huan.quickdeveloplibrary.view.tabmenu.TabMenuLayout
        android:id="@+id/tab_title_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:gravity="center"
        android:layout_gravity="center"
        android:visibility="gone">

    </cn.demomaster.huan.quickdeveloplibrary.view.tabmenu.TabMenuLayout>

<cn.demomaster.huan.quickdeveloplibrary.widget.ImageTextView
        android:id="@+id/it_actionbar_common_right"
        android:layout_width="@dimen/activity_actionbar_button_width"
        android:layout_height="match_parent"
        android:layout_gravity="right"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="@dimen/quickdev_title_text_size"
        android:gravity="center_vertical" />
<!--
<View android:layout_width="match_parent"
      android:layout_height="2dp"
      android:layout_gravity="bottom"
      android:background="#65EDEDED"
      />-->
    <!--android:background="@drawable/actionbar_line_shaper"-->
</FrameLayout>
