<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cn.demomaster.huan.doctorbaselib">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/StartAppTheme"
        tools:replace="android:theme,android:allowBackup">
        <activity android:name="cn.demomaster.huan.doctorbaselibrary.view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".authorization.AuthorizationStatementActivity">
        </activity>
        <activity android:name=".authorization.AuthenticationPersonActivity">
        </activity>
        <activity android:name=".authorization.AuthorizationActivity">
        </activity>

    </application>

</manifest>